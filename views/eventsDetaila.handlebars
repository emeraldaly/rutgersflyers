  <div class="container-fluid ">
    <div class="row-fluid">
      <div class="venuename col-md-6 col-md-offset-3">
        {{#each Venues}}
        <div class="text-center">
          <h2 class="detitle">{{name}}</h2>
            <p>{{this.address}} </p>
            <p>{{this.phoneNumber}}</p>
            <p>{{this.website}}</p>
            <p>{{this.date}}</p>
            <p>{{this.time}}</p>
          </div>
               {{#each Reviews}}
               <div class ="reviewDiv">
               <p>{{this.review}} </p>
                <p class="rating text-right">Rating: {{this.rating}} </p>
          </div>
               {{/each}}
                <form class="reviewForm"method="POST" onsubmit="return validateReview()"action="/review/{{ id }}">
               <h4>Review: </h4>
              <textarea id="review" rows="4" cols="50" type="text" class="form-control"
               name="review"> </textarea>
              <h4>Rating: </h4>
              <div class="form-group">
            <label class="checkbox-inline"><input type="radio" name="rating" value="1"> 1</label>
          <label class="checkbox-inline"><input type="radio" name="rating"
            value="2"> 2</label>
          <label class="checkbox-inline"><input type="radio" name="rating" value="3"> 3</label> 
          <label class="checkbox-inline"><input type="radio" name="rating" value="4"> 4</label> 
          <label class="checkbox-inline"><input type="radio" name="rating" value="5"> 5</label>
            </div>
   
              <button type="submit" class="btn btn-default dataButton">Submit</button>
              <div class="validateReview"> </div>
            </form>
            <button class="createReview btn btn-default">write a review </button>
       {{/each}}     
               
               <br />
        </div>
      </div> 
    </div>
</div>
</div>
</div>

