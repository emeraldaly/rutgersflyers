<body>
  <div class="container">
    <div class="row">
      <div class="col-md-3">
      </div>
      <div class="venuename col-md-6">
        <br>
        {{#each Venues}}
        <h2 class="detitle">{{name}}</h2>
               <p>{{this.address}} </p>
               <p>{{this.phoneNumber}}</p>
               <p>{{this.website}}</p>
          
               {{#each Reviews}}
               <p>{{this.review}}
               <p>Rating: {{this.rating}}
               {{/each}}
               <form class="reviewForm"method="POST" action="/review/{{ id }}">
              <input type="text" class="form-control"  placeholder="review" name="review">
              <div class="form-group">
            <label class="checkbox-inline"><input type="radio" name="test" value="stud">Student</label>
          <label class="checkbox-inline"><input type="radio" name="test"
            value="teacher">Teacher</label>
          <label class="checkbox-inline"><input type="radio" name="test" value="ta">TA</label>
                        </div>
                        

              <button type="submit" class="btn btn-default dataButton">submit review</button>
            </form>
            <button class="createReview btn btn-default">write a review </button>
       {{/each}}     
               
               <br />
        </div>
      </div> 
    </div>






</div>
</div>
</div>
</body>
