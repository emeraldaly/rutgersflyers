<body>
  <div class="container">
    <div class="row">
      <div class="col-md-3">
        <!-- Nav tabs -->
        <ul class="nav icons nav-tabs">
          <li class="active"><a href="/food">Food<div><img src="/img/spotfood.png" class="img1"></div></a></li>
          <li><a href="/events" 
                 >Events<div> <img
                 src="/img/spotcalendar.png" class="img1"></div></a></li>
          <li><a href="/transportation"
                 >Transportation<div> <img
                 src="/img/spottransportation.png" class="img1"> </div> </a></li>
          <li><a href="/services" 
                 >Services<div><img
                 src="/img/spotservices.png" class="img1"></div> </a></li>
        </ul>  
      </div>
      <div class="venuename col-md-6">
        <h4>Food</h4>
        <br>
        {{#each Venues}}
        <a class="title" href="/food/{{this.id}}"><h4>     {{this.name}}</h4></a>
               <p>
               {{#each Reviews}}
               {{this.review}}...<span
           class="rating">Rating: </span>{{this.rating}}
         <p> {{/each}}
{{/each}}
          <br />
        </div>
      </div> 
    </div>




</body>
